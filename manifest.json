{
	"manifest_version": 2,
	"name": "无缓存模式",
	"version": "1.0.0",
	"description": "无缓存模式",
	"icons":
	{
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png"
	},
	"browser_action":
	{
		"default_icon": "img/icon.png",
		"default_title": "无缓存模式",
		"default_popup": "popup.html"
	},
	"permissions":
	[
		"declarativeContent",
		"storage",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	],
	"background":
	{
		"scripts": ["js/background.js"],
		"persistent": true
	}
}
